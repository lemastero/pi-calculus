let triple(a,b,c) = pair(a,pair(b,c))
let print(msg) = out(stdout,msg)
let error(msg) = out(stderr,msg)
let dbgOut(c, msg) = out(c,msg);out(stdout,msg)
let fetch_out(c) = in(stdin,msg);out(c,msg)
let in_print(c)  = in(c,msg);out(stdout,msg)
let in_and_out(c) = &in_print(c);&fetch_out(c)
let out_and_in(c) = &fetch_out(c);&in_print(c) 
let forever(process) = &process ; &forever(process)
let getLocation(data) = getHeader("location",data)
